[
    {{- $entries := slice -}}
    {{- range .Site.AllPages -}}
        {{- if and (ne .Kind "section") (ne .Layout "search") (ne .Layout "archives") (ne .Title "") -}}
            {{- $entry := dict "title" .Title "content" .Plain "permalink" .Permalink -}}
            {{- $entries = $entries | append $entry -}}
        {{- end -}}
    {{- end -}}
    {{- range $index, $entry := $entries -}}
        {{- if $index -}},{{- end -}}
        {{- $entry | jsonify -}}
    {{- end -}}
]
